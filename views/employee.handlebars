  <div class="container">
    <div class="section">

      <!--   Category Selection -->
      <div class="row">
        <div class="col s12 m12">

          <select id="category-search">
            <option value="" disabled selected>Choose your job Category</option>
            <option value="Maintainence">Maintainence</option>
            <option value="Manual Labor">Manual Labor</option>
            <option value="Miscellaneous">Miscellaneous</option>
          </select>
          <br>
        </div>

        <div class="row">
          <div class="row">
            <button id="search-btn" class="btn-large">Find My Job!</button>
            <br>
            <br>
            <br>
            <table class="highlight" id='jobtable'>
              <thead>
                <tr>
                  <th>Employer Name</th>
                  <th>Category</th>
                  <th>Description</th>
                  <th>Date</th>
                  <th>Time</th>
                  <th>Amount</th>
                  <th> Select</th>
                </tr>
              </thead>
              <tbody></tbody>
            </table>
          </div>
        </div>
      </div>
    </div>

  </div>

